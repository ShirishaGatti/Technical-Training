class Solution {
    public int maxPower(String s) {
        int i=0,j=1,max=1;
        while(j<s.length()){
            if(s.charAt(i)==s.charAt(j)){
                max=Math.max(max,j-i+1);
            }
            else{
                i=j;
            }
            j++;
        }
        return max;
    }
}